
<% title "Login" %>


<%= bootstrap_form_with(url: sessions_path, method: :post, local: true, label_col: "col-sm-2", control_col: "col-sm-10") do |f| %>
  <%= f.email_field :email %>
  <%= f.password_field :password %>
  <%= f.primary "Login"%>
<% end %>

OR <%= link_to "Login with Github", "/auth/github" %>